{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "web.core.ui"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "api.config.js"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.css",
        "styles.css",
        "../node_modules/sweetalert2/dist/sweetalert2.min.css",
        "../node_modules/print.js/dist/print.min.css",
        "./assets/css/login.css",
        "./assets/vendors/custom/fullcalendar/fullcalendar.bundle.css",
        "./assets/vendors/base/vendors.bundle.css",
        "./assets/demo/demo3/base/style.bundle.css"
      ],
      "scripts": [
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "../node_modules/sweetalert2/dist/sweetalert2.min.js",
        "../node_modules/moment/min/moment.min.js",
        "../node_modules/moment/min/moment-with-locales.min.js",
        "../node_modules/lodash/lodash.min.js",
        "./assets/vendors/base/vendors.bundle.js",
        "../node_modules/bootstrap-notify/bootstrap-notify.min.js",
        "./assets/demo/demo3/base/scripts.bundle.js",
        "./assets/js/main.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
